{
  "path" : {
    "application" : {
        "root" : "application",
        "pages" : "application/pages/",
        "fonts" : "application/assets/fonts/",
        "images" : "application/assets/images/",
        "js" : "application/assets/js/",
        "pdf" : "application/assets/pdf/",
        "scss" : "application/assets/scss/"
    },
    "distribution" : {
        "root" : "distribution",
        "fonts" : "distribution/assets/fonts/",
        "images" : "distribution/assets/images/",
        "js" : "distribution/assets/js/",
        "pdf" : "distribution/assets/pdf/",
        "css" : "distribution/assets/css/"
    }
  },
  "autoprefixeroptions" : {
    "browsers" : [
      "last 5 versions",
      "ie >= 10"
    ]
  },
  "imageoptions" : {
    "optimizationlevel" : {
      "png" : 3,
      "jpg" : 3
    }
  }
